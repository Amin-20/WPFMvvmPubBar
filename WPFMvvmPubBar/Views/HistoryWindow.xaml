<Window x:Class="WPFMvvmPubBar.Views.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFMvvmPubBar.Views"
        mc:Ignorable="d"
        Title="HistoryWindow" Height="420" Width="408">
    <Canvas>
        <GroupBox Header="Sales" FontSize="30" Height="329" Width="373" HorizontalAlignment="Left" Canvas.Left="10" VerticalAlignment="Center" >
            <ItemsControl ItemsSource="{Binding Sales }" Margin="0,0,0,32"></ItemsControl>
        </GroupBox>
        <Button Command="{Binding OkCommand}" Width="100" Height="26" Canvas.Left="263" Canvas.Top="345" Content="OK" FontSize="17"/>
    </Canvas>
</Window>
